# Esempio di configurazione per ambiente di produzione

# App Configuration
VITE_APP_NAME="Biblioteca Digitale Pyragogica"
VITE_APP_URL="https://pyragogy.org"
VITE_APP_VERSION="1.0.0"

# Analytics (opzionale)
VITE_GA_MEASUREMENT_ID="G-XXXXXXXXXX"

